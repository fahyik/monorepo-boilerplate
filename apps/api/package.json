{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "TZ=UTC NODE_ENV=development DOTENV_CONFIG_PATH=./.env.development.local nodemon --delay 0.7 --exec \"tsx --inspect=0.0.0.0:9229 --no-cache --require dotenv/config ./src/index.ts\" -e .ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\" --max-warnings 0",
    "start": "TZ=UTC NODE_ENV=production DOTENV_CONFIG_PATH=./.env.production node -r dotenv/config ./dist/index.js",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "@packagename/jest-presets/node"
  },
  "dependencies": {
    "@packagename/logger": "*",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "knex": "^3.1.0",
    "mailgun.js": "^12.0.1",
    "multer": "^1.4.5-lts.2",
    "pg": "^8.14.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@packagename/eslint-config": "*",
    "@packagename/jest-presets": "*",
    "@packagename/typescript-config": "*",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.10",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.22.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
