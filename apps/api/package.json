{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "TZ=UTC NODE_ENV=development DOTENV_CONFIG_PATH=./.env.development.local nodemon --delay 0.7 --exec \"tsx --inspect=0.0.0.0:9229 --no-cache --require dotenv/config ./src/index.ts\" -e .ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\" --max-warnings 0",
    "start": "TZ=UTC NODE_ENV=production DOTENV_CONFIG_PATH=./.env.production node -r dotenv/config ./dist/index.js",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "@packagename/jest-presets/node"
  },
  "dependencies": {
    "@packagename/logger": "*",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "knex": "^3.1.0",
    "pg": "^8.16.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@packagename/eslint-config": "*",
    "@packagename/jest-presets": "*",
    "@packagename/typescript-config": "*",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.18.0",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.31.0",
    "jest": "^30.1.1",
    "nodemon": "^3.1.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3"
  }
}
